<main class="container">
  <header>
    <h1>記事一覧</h1>
  </header>
  <%= link_to "新しい記事を作成する", new_article_path, class: "btn btn-default" %>
  <% @articles.each do |article| %>
  <article>
    <h2><%= article.title %></h2>
    <small><%= article.created_at %></small>
    <p><%= article.content %></p>
  </article>
  <% end %>
</main>